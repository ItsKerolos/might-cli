{
  "name": "might-cli",
  "version": "1.3.0",
  "description": "A no-code solution for performing end-to-end tests",
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf ./build && mkdir ./build && babel -d ./build ./src",
    "watch": "rm -rf ./build && mkdir ./build && babel -d ./build src -s --watch",
    "start": "node ./build/cli.js"
  },
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "might": "build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ker0olos/might-cli.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ker0olos/might-cli/issues"
  },
  "homepage": "https://github.com/ker0olos/might-cli#readme",
  "keywords": [
    "testing",
    "coverage",
    "coverage-reports",
    "end-to-end-testing",
    "ui-testing",
    "front-end-testing",
    "end-to-end-test-automation",
    "end-to-end-testing-interface"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "convert-source-map": "^1.7.0",
    "draftlog": "^1.0.12",
    "exit": "^0.1.2",
    "fs-extra": "^9.1.0",
    "is-ci": "^2.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-report": "^3.0.0",
    "istanbul-reports": "^3.0.2",
    "jimp": "^0.16.1",
    "looks-same": "^7.3.0",
    "md5": "^2.3.0",
    "might-core": "^0.3.8",
    "minimist": "^1.2.5",
    "nanomatch": "^1.2.13",
    "node-fetch": "^2.6.1",
    "p-limit": "^3.1.0",
    "prompts": "^2.4.0",
    "ps-tree": "^1.2.0",
    "puppeteer": "^7.0.1",
    "sanitize-filename": "^1.6.3",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
