{
  "name": "might-cli",
  "version": "0.0.1",
  "description": "A simple way to perform end-to-end tests",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf ./build && mkdir ./build && babel -d ./build ./src",
    "watch": "rm -rf ./build && mkdir ./build && babel -d ./build src -s --watch",
    "start": "node ./build/index.js"
  },
  "bin": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsKerolos/Might.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsKerolos/Might/issues"
  },
  "homepage": "https://github.com/ItsKerolos/Might#readme",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "fs-extra": "^9.0.0",
    "puppeteer": "^2.1.1",
    "terminal-kit": "^1.35.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
