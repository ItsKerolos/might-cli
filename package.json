{
  "name": "might-cli",
  "version": "0.2.5",
  "description": "A no-code solution for performing end-to-end tests",
  "scripts": {
    "prepare": "npm run build",
    "postpublish": "npm version patch --no-git",
    "build": "rm -rf ./build && mkdir ./build && babel -d ./build ./src",
    "watch": "rm -rf ./build && mkdir ./build && babel -d ./build src -s --watch",
    "start": "node ./build/cli.js"
  },
  "bin": {
    "might": "build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsKerolos/might-cli.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsKerolos/might-cli/issues"
  },
  "homepage": "https://github.com/ItsKerolos/might-cli#readme",
  "keywords": [
    "test",
    "testing",
    "ui",
    "ui-testing",
    "end-to-end-test",
    "end-to-end-test-automation",
    "end-to-end-testing"
  ],
  "dependencies": {
    "@babel/runtime": "^7.11.0",
    "fs-extra": "^9.0.1",
    "might-core": "^0.2.4",
    "pixelmatch": "^5.2.1",
    "pngjs": "^5.0.0",
    "ps-tree": "^1.2.0",
    "puppeteer": "^5.2.1",
    "terminal-kit": "^1.42.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
